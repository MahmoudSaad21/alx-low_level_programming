#include <stdio.h>
/**
*main - prints out first 50
*fibonacci suit numbers
*Return: return 0
*/
int main(void)
{
int count;
unsigned long n1 = 0, n2 = 1, n3, sum = 0;
for (count = 0; count < 40000; count++)
{
if (n3 >= 4000000)
break;
n3 = n1 + n2;
n1 = n2;
n2 = n3;
if (n1 % 2 == 0)
sum = sum +n1;
}
 
printf("%ld\n", sum);
return (0);
}
