#include "main.h"
/**
*_strcpy -  prints every other character of a string
*@dest: first num
*@src: first num
*Return: nothing
*/
char *_strcpy(char *dest, char *src)
{
int count = 0;
while (*(src + count) != '\0')
{
*(dest + count) = *(src + count);
count++;
}
*(dest + count) = '\0';
return (dest);
}
